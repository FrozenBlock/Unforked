{
  "schemaVersion": 1,
  "id": "unforked",
  "version": "${version}",

  "name": "Unforked",
  "description": "A compatibility layer to run Quilt mods on Fabric",
  "authors": [
    "FrozenBlock"
  ],
  "contact": {
    "homepage": "https://github.com/FrozenBlock/Unforked",
    "sources": "https://github.com/FrozenBlock/Unforked",
    "issues": "https://github.com/FrozenBlock/Unforked/issues"
  },

  "license": "LGPL-3.0",
  "icon": "assets/unforked/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.frozenblock.unforked.Unforked"
    ],
    "client": [
      "net.frozenblock.unforked.UnforkedClient"
    ],
    "fabric-datagen": [
      "net.frozenblock.unforked.datagen.UnforkedDataGenerator"
    ]
  },
  "mixins": [
    "unforked.mixins.json"
  ],
  "accessWidener": "unforked.accesswidener",

  "depends": {
    "frozenlib": ">=1.1.11-",
    "fabric-language-kotlin": "${fabric_kotlin_version}",
    "fabricloader": ">=0.14.12",
    "fabric-api": ">=0.73.0",
    "minecraft": ">=${minecraft_version}",
    "java": ">=21"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/EpkBWm844s"
      },
      "badges": [
        "frozenblock"
      ]
    }
  }
}
